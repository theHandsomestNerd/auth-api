{
  "root": true,
  "api": {
    "projectId": "abtncxbi",
    "dataset": "development"
  },
  "project": {
    "name": "kookout"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
